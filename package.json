        "laravel-mix": "^6.0.43",
        "lodash": "^4.17.21",
        "popper.js": "^1.12",
        "postcss": "^8.4.12",
        "prop-types": "^15.8.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-i18next": "^11.16.2",
        "react-router-dom": "^5.3.0",
        "resolve-url-loader": "^3.1.4",
        "sass": "^1.49.9",
